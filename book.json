{
    "plugins": [
        "-tbfed-pagefooter",
        "github",
        "theme-default",
        "emphasize",
        "include-codeblock",
        "ace",
        "numbered-headings",
        "-signature",
        "-theme-api"
    ],
    "pluginsConfig": {
        "fontSettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 1
        },
        "theme-default": {
             "showLevel": true
        },
        "github": {
            "url": "https://github.com/lynnhawk/pthinkbook.git"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy www.pthink.com.cn 2016",
            "modify_label": "modified: ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    },
    "author": "phy",
    "pdf": {
         "fontSize": 12,
        "paperSize": "a4",
        "pageNumbers": false,
        "margin": {
            "left": 62,
            "right": 62,
            "top": 36,
            "bottom": 36
        },
        "toc": true
    },
    "title": "产品开发指导书",
    "description": "产品开发指导书API"
}